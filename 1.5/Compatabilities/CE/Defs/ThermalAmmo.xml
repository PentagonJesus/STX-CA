	<!-- ==================== Recipes ==================== -->
		<!-- Thermal Bolts -->
	<RecipeDef ParentName="ChargeAmmoRecipeBase">
		<defName>MakeAmmo_ThermalBolts</defName>
		<label>make Thermal Bolts cartridge x500</label>
		<description>Craft 500 Thermal Bolts cartridges.</description>
		<jobString>Making Thermal Bolts cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>18</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Uranium</li>
					</thingDefs>
				</filter>
				<count>16</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Prometheum</li>
					</thingDefs>
				</filter>
				<count>14</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>Uranium</li>
				<li>Prometheum</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_ThermalBolts>500</Ammo_ThermalBolts>
		</products>
		<workAmount>16000</workAmount>
	</RecipeDef>

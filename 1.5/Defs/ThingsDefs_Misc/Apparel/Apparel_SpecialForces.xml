<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- crusader division -->
	<VFEPirates.WarcasketDef ParentName="VFEP_WarcasketArmorBase">
		<defName>SF_CrusaderDivision</defName>
		<label>501st Crusader Division armor</label>
		<shortDescription>This Warcasket is only entrusted to warriors who dedicate their whole lives to the emperor and the empire.</shortDescription>
		<description>This Warcasket is only entrusted to warriors who dedicate their whole lives to the emperor and the empire.  With a powerful jet pack and nearly impenetrable armor that few things can pierce, many have come to fear the sight of this armor.  While this armor is a rare sight it is most commonly issued to troops on key worlds or on the frontlines of very important battles.</description>
		<graphicData>
			<texPath>Things/Pawn/Warcasketlike/HiggsCasket</texPath>
		</graphicData>
		<statBases>
			<Mass>75</Mass>
			<ArmorRating_Sharp>1.56</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.65</ArmorRating_Blunt>
			<ArmorRating_Heat>0.78</ArmorRating_Heat>
			<Insulation_Cold>100</Insulation_Cold>
			<Insulation_Heat>14</Insulation_Heat>
			<EquipDelay>1</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<VEF_MassCarryCapacity>125</VEF_MassCarryCapacity>
			<MoveSpeed>0.60</MoveSpeed>
			<SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">0.3</SlaveSuppressionOffset>
		</equippedStatOffsets>
		<costList>
			<ComponentSpacer>8</ComponentSpacer>
			<Plasteel>300</Plasteel>
			<DevilstrandCloth>120</DevilstrandCloth>
			<Uranium>50</Uranium>
		</costList>
		<researchPrerequisites>
			<li>VFEP_SpacerWarcaskets</li>
		</researchPrerequisites>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Legs</li>
				<li>Feet</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Warcasketlike/HiggsCasket</wornGraphicPath>
			<layers>
				<li>OnSkin</li>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>WarcasketHeavy</li>
				<li>WarcasketCata</li>
				<li>WarcasketAll</li>
			</tags>
		</apparel>
		<modExtensions>
			<li Class="VFECore.ApparelExtension">
				<traitsOnEquip>
					<li>VFEP_WarcasketTrait</li>
				</traitsOnEquip>
				<traitsOnUnequip>
					<li>VFEP_Shellcasket</li>
				</traitsOnUnequip>
				<pawnCapacityMinLevels>
					<li>
						<capacity>Moving</capacity>
						<minLevel>0.7</minLevel>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<minLevel>0.7</minLevel>
					</li>
				</pawnCapacityMinLevels>
			</li>
		</modExtensions>
	</VFEPirates.WarcasketDef>

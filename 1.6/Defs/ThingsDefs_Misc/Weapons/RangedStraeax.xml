<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--Chazos L3-->
	
	<ThingDef ParentName="BaseHumanMakeableGun">
		<defName>Gun_ChazosLThree</defName>
		<label>Chazos L3 Pistol</label>
		<description>The Chazos L3 is the standard issue sidearm for all Straeax Operatives, soldiers and peace-keeping forces. The Chazos L3 is a semi-automatic pistol featuring an integrated silencer and laser, fitted with an advanced sight picture system it is a well used and love sidearm.</description>
		<techLevel>Spacer</techLevel>
		<graphicData>
			<texPath>Things/Item/Equipment/WeaponRanged/ChazosL3</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Interact_ChargeRifle</soundInteract>
		<statBases>
			<WorkToMake>25000</WorkToMake>
			<Mass>1</Mass>
			<AccuracyTouch>0.55</AccuracyTouch>
			<AccuracyShort>0.64</AccuracyShort>
			<AccuracyMedium>0.55</AccuracyMedium>
			<AccuracyLong>0.45</AccuracyLong>
			<RangedWeapon_Cooldown>0.2</RangedWeapon_Cooldown>
		</statBases>
		<equippedStatOffsets>
			<ShootingAccuracyPawn>0.05</ShootingAccuracyPawn>
			<AimingDelayFactor>-0.1</AimingDelayFactor>
		</equippedStatOffsets>
		<recipeMaker>
			<researchPrerequisite>StraeaxArmory</researchPrerequisite>
			<skillRequirements>
				<Crafting>10</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="False">
				<li>FabricationBench</li>
			</recipeUsers>
			<displayPriority>201</displayPriority>
		</recipeMaker>
		<costList>
			<Uranium>5</Uranium>
			<Plasteel>50</Plasteel>
			<ComponentSpacer>2</ComponentSpacer>
			<VoltheriumHexTubes>1</VoltheriumHexTubes>
		</costList>
		<thingSetMakerTags>
			<li>RewardStandardQualitySuper</li>
		</thingSetMakerTags>
		<weaponTags>
			<li>SpacerGun</li>
		</weaponTags>
		<tradeTags>
			<li>WeaponRanged</li>
		</tradeTags>
		<weaponClasses>
			<li>RangedLight</li>
		</weaponClasses>
		<verbs>
			<li>
				<verbClass>Verb_Shoot</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>Bullet_ThermalBoltNonCE</defaultProjectile>
				<warmupTime>0.3</warmupTime>
				<range>26</range>
				<soundCast>Shot_ChargeRifle</soundCast>
				<soundCastTail>GunTail_Medium</soundCastTail>
				<muzzleFlashScale>9</muzzleFlashScale>
			</li>
		</verbs>
		<tools>
			<li>
				<label>stock</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>9</power>
				<cooldownTime>2</cooldownTime>
			</li>
			<li>
				<label>muzzle</label>
				<capacities>
					<li>Blunt</li>
					<li>Poke</li>
				</capacities>
				<power>9</power>
				<cooldownTime>2</cooldownTime>
			</li>
		</tools>
	</ThingDef>

